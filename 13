#8
a = set(input())
b = set(input())
c = set(input())
print(c.issubset(a | b))
from collections import Counter
n = int(input())
emps = [set(input().split()) for _ in range(n)]
cnt = Counter(skill for e in emps for skill in e)
print(sorted([s for s,v in cnt.items() if v==1]))
