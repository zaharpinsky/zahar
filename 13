vowels = set('аеёиоуыэюя')
res = sorted({c for c in text if c in vowels})
print(res)
from collections import Counter
m = int(input())
prods = [set(input().split()) for _ in range(m)]
cnt = Counter(cat for p in prods for cat in p)
print(sorted([c for c,v in cnt.items() if v==1]))
