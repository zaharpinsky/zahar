#11
text = input().lower()
vowels = set('аеёиоуыэюя')
letters = [c for c in text if c.isalpha()]
v = sum(1 for c in letters if c in vowels)
c = len(letters) - v
print('vowels' if v>c else 'consonants' if c>v else 'equal')
n = int(input())
days = [set(input().split()) for _ in range(n)]
d = int(input())-1
others = set().union(*(days[:d]+days[d+1:])) if n>1 else set()
print(sorted(days[d] - others))
